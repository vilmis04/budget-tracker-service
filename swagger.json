{
  "openapi": "3.0.0",
  "info": {
    "title": "Express API with Swagger",
    "version": "1.0.0",
    "description": "A simple API documentation example using Swagger and Express."
  },
  "servers": [
    {
      "url": "http://localhost:8000",
      "description": "Local server"
    }
  ],
  "paths": {
    "/api/health": {
      "get": {
        "summary": "Return if the service is alive",
        "responses": {
          "200": {
            "description": "Alive if the service is alive",
            "content": {
              "text/plain": {
                "schema": {
                  "type": "string"
                }
              }
            }
          }
        }
      }
    }
  }
}
